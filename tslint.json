{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "ordered-imports": false,
    "no-console": false,
    "interface-name": false,
    "only-arrow-functions": false
  },
  "rulesDirectory": []
}